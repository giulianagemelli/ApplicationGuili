<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity2">

    <ImageButton
        android:id="@+id/INFOBOTON"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/botonredondo"
        android:onClick="Inodoro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/informacion" />

    <ImageButton
        android:id="@+id/INFOBOTON2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/botonredondo"
        android:onClick="TipodeCasa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/INFOBOTON"
        app:srcCompat="@drawable/informacion" />
</androidx.constraintlayout.widget.ConstraintLayout>